
import java.util.*;

public class MainEntry {

	public static void main(String[] args) {
		String title = null; // 비디오 제목
		String category = null; // 장르
		boolean lend = false; // 1 : 대여가능, 0 : 대여불가
		String lendName; // 대여자(고객명)
		String lendDate; // 대여일자(오늘날짜)
		
		Scanner sc = new Scanner(System.in);
		VideoManager vid = new VideoManager();
		
		
		
		int check;
			while(true) {
				menu();
				
				switch (check = sc.nextInt()) {
				case 1: // 추가
					add();
					vid.VideoAdd(title, category, lend);
					break;
				case 2: // 삭제
					del();
					break;
				case 3: // 출력
					show();
					break;
				case 4: // 수정
					update();
					break;
				default:
					System.out.println("다시 입력해주세요.");
					break;
				}
			} // while end
		
		

	}
	
	// case 1: Video 추가
	public static void add() {
		Scanner sc = new Scanner(System.in);
		
		title = sc.next();
	}
	
